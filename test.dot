digraph G{

	graph [label = "\n\nGraph Title"];

	rankdir=LR;
	node [style=filled]
	a -> b -> c -> a
		
	subgraph clusterA{
		node [fillcolor=azure]
		label="Cluster A"
		inhabitant1 ->
		inhabitant2
	}
	
		subgraph clusterShapes{
		label=Shapes
		
		house [shape=house]
		rotatedHouse [shape=house orientation=45]
	}

	description [
	      shape=note
	      fontsize=12
	      fillcolor=wheat
	      label="How the components \ninteract with each other"
	    ]

	
}